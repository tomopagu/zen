{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["eslint-config-airbnb", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    },
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "prettier/prettier": "error"
  }
}
